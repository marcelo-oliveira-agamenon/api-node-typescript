datasource db {
  provider = "postgresql"
  url = "***"
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id  String  @default(uuid()) @id
  name String
  email String @unique
  password String
  imageUrl String
  role Role
  createdAt DateTime @default(now())
  modifiedAt DateTime @default(now())
}

enum Role {
  USER
  ADMIN
}