{
  "compilerOptions": {
    "target": "es2017",
    "sourceMap": true,
    "allowJs": true,
    "lib": ["ES6"],
    "removeComments": true,
    "module": "commonjs",
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  }
}
